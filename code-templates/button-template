class Component extends React.Component {
    render() {
        return (<Button id="${componentIdentifier}" variant="contained" onClick={this.props.onClick} disabled={this.props.disabled} >
                    {this.props.value}
                </Button>);
    }

    shouldComponentUpdate(nextProps, nextState){
        return this.props.disabled !== nextProps.disabled ||
            this.props.value !== nextProps.value ||
            this.props.clickAction !== nextProps.clickAction;
    }
}