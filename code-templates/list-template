const Child = (props) => {
    return (${childComponent})
};

class Component extends React.Component {
    render() {
        return (
            <List>
                {(this.props.value || []).map((v, index) => {
                    return (<ListItem>
                        <Child item={v} index={index}/>
                    </ListItem>)
                    })
                }
            </List>
        );
    }
}