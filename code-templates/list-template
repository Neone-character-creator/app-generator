const Child = (props) => {
    return (${childComponent})
};

const Component = (props) => (
    <List>
        {(props.value || []).map((v, index) => {
            var childContext = {...props.context, ...{
                $index: index}};
            return (<ListItem>
                <Child value={v} context={childContext} />
            </ListItem>)
            })
        }
    </List>
);