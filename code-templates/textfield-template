import React from "react";
import { connect } from "react-redux";
import Textfield from "@material-ui/core/TextField";
%imports%

const Component = () => (
    <Textfield id="%name%-textfield" label="%label%" value="{props.boundValue}" />
);

const boundPropertyName = "%bind%".split(".");

const connected = connect((state) => {
    const boundValue = boundPropertyName.reduce((currentValue, nextSegment) => {
        return currentValue[nextSegment];
    });
    return {
        boundValue
    };
});

export default Component;